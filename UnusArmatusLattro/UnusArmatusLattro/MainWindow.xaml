<Window x:Class="UnusArmatusLattro.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UnusArmatusLattro"
        mc:Ignorable="d"
        Title="MainWindow" Height="650" Width="1100">
    <Window.InputBindings>
        <KeyBinding Command="{Binding CurrentViewModel.Spin}" Key="Space">
            
        </KeyBinding>
    </Window.InputBindings>
    <Grid Background="#FFBEC3">
        <ContentControl Content="{Binding CurrentViewModel}" />
        <!--<Image Source="/Resources/Images/lemon.png"></Image>-->
    </Grid>
</Window>
